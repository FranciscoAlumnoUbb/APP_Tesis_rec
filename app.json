{
  "expo": {
    "name": "RecyclAIDeep",
    "slug": "recyclaideep-tesis-2025",
    "version": "1.0.0",
    "owner": "yalersin",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "recyclaideep",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.yalersin.recyclaideep",
      "usesCleartextTraffic": true,
      "permissions": [
        "CAMERA",
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "ACCESS_WIFI_STATE",
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDByHWYJ-mMy3_rKfNAYIpxzliAR8orJSA"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-web-browser"
    ],
    "assetBundlePatterns": [
      "assets/raw_model/*",
      "assets/images/*"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b56e8337-adf8-440d-931c-27db463d2323"
      }
    }
  }
}
